server <- function(input, output){
datasetInput <- reactive({
switch(input$dataset,
"prenatal" = prenatal,
"infections" = infections,
"congenital" = congenital,
"blood" = blood,
"circulatory" = circulatory,
"digestive" = digestive,
"genitourinary" = genitourinary,
"musuloskeletal" = musculoskeletal,
"nervous" = nervous,
"respiratory" = respiratory,
"skin" = skin,
"endocrine"  = endocrine,
"external" = external,
"mental" = mental,
"neoplasms" = neoplasms,
"pregnancy" = pregnancy,
"other" = other
)
})
}
